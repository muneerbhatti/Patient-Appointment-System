<div class="content-body"></div>
<div class="col-lg-12">
  <div class="card">
    <div class="card-body">
      <h1 class="text-center">Token Detail</h1>
    </div>  
  </div>

  <div class="p-4">
    <p class="lead"><strong>Blood Pressure:</strong> <%= @token.Blood_pressure %></p>
    <p class="lead"><strong>Temperature:</strong> <%= @token.Temperature %></p>
    <p class="lead"><strong>Weight:</strong> <%= @token.Weight %></p>
    <p class="lead"><strong>Doctor ID:</strong> <%= @token.doctor_id %></p>
    <p class="lead"><strong>Patient ID:</strong> <%= @token.patient_id %></p>
  </div>

  <div class="mt-4">
    <%= link_to 'Back to Tokens', tokens_path, class: 'btn btn-primary btn-lg' %>
    <%= link_to 'Edit Token', edit_token_path(@token), class: 'btn btn-info btn-lg' %>
  </div>
</div>
<style type="text/css">
     .content-body {
        margin-left:900px;
    }
</style>