<div class="col-lg-12">
	<div class="card">
		<div class="card-body">
			<h1 class="text-center">Doctor Infermation</h1>
		</div>
	</div>
	<table class="table table-borderd">
		<thead>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Email</th>
				<th>Conect</th>
				<th>Address</th>
				<th>Specialization</th>
				<th>Eprince</th>
			</tr>
		</thead>
		<tbody>
			<% @doctors.each do |doctor| %>
			<tr>
				<td><%=doctor.id %></td>
				<td><%=doctor.Name %></td>
				<td><%=doctor.Email %></td>
				<td><%=doctor.ConectNo %></td>
				<td><%=doctor.Address%></td>
				<td><%=doctor.Specialization %></td>
				<td><%=doctor.Exprince_year%></td>
		
			</tr>
			<% end %>
		</tbody>
	</table>
</div>