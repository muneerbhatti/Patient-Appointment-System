<div class="col-lg-12">
	<div class="card">
		<div class="card-body">
			<h1 class="text-center">Patient Detial</h1>
		</div>	
	</div>
	<p class="lead"><strong>ID = </strong><%=@patient.id %> </p>
	<p class="lead"><strong>Name = </strong><%=@patient.Name %> </p>
	<p class="lead"><strong>CNIC = </strong><%=@patient.CNIC %> </p>
	<p class="lead"><strong>Email = </strong><%=@patient.email %> </p>
	<p class="lead"><strong>PhoneNo = </strong><%=@patient.PhoneNo %> </p>
	<p class="lead"><strong>Gender = </strong><%=@patient.Gender%> </p>
	<p class="lead"><strong>date_of_birth = </strong><%=@patient.date_of_birth %> </p>
	<p class="lead"><strong>Address = </strong><%=@patient.Address %> </p>
	<%=link_to 'Back to patient',patients_path,class:'btn btn-primary btn-lg ' %>
	<%=link_to 'Edit patient',edit_patient_path,class:'btn btn-info btn-lg ' %>
	</div>